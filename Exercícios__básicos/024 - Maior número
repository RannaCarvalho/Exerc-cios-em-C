#include <stdio.h>

int main()
{
    int numero1, numero2, numero3;
    int maior_numero;
    
    printf("Digite tres numeros inteiros: ");
    scanf ("%d %d %d", &numero1, &numero2, &numero3);
    
    if (numero1>numero2 && numero1>numero3) maior_numero = numero1;
    if (numero2>numero1 && numero2>numero3) maior_numero = numero2;
    if (numero3>numero1 && numero3>numero2) maior_numero = numero3;
    
    printf ("maior numero: %d", maior_numero);

    return 0;
}
