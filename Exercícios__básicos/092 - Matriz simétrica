#include <stdio.h>
#include <locale.h>
#define MAX 5

int main(){
    setlocale(LC_ALL, "Portuguese");
    int matriz[MAX][MAX];
    int simetrico=1;
    int i=0, j=0;
    
    printf ("Digite os valores da matriz:\n");
    for(i = 0; i < MAX; i++){
		for(j = 0; j < MAX; j++){
			printf("Insira o valor da posição [%d, %d]: ", i, j);
			scanf("%d", &matriz[i][j]);
		}
	}
    for (i=0; i<MAX; i++){
        for (j=0; j<MAX; j++){
            if (matriz[i][j]!=matriz[j][i]){
                simetrico = 0;
                break;
            }
        }
        if (simetrico == 0) {
            break;
        }
    }
    if (simetrico == 1) {
        printf("\nA matriz é simétrica.\n");
    } else {
        printf("\nA matriz não é simétrica.\n");
    }
    return 0;
}
